	"scripts": {
		"dev": "concurrently \"npx tailwindcss -i ./src/tailwind-from.css -o ./src/tailwind-to.css --watch\" \"vite\"",
		"build": "tsc -b && npx tailwindcss -i ./src/tailwind-from.css -o ./src/tailwind-to.css && npx --max_old_space_size=10240 vite build",
