{
	"name": "briar-frontend",
	"private": true,
	"version": "0.0.9",
	"type": "module",
	"scripts": {
		"dev": "concurrently \"npx tailwindcss -i ./src/tailwind-from.css -o ./src/tailwind-to.css --watch\" \"vite\"",
		"build": "tsc -b && npx tailwindcss -i ./src/tailwind-from.css -o ./src/tailwind-to.css && vite build",
		"build:quick": "tsc -b && npx tailwindcss -i ./src/tailwind-from.css -o ./src/tailwind-to.css && vite build --mode quick",
		"build:watch": "tsc -b && npx tailwindcss -i ./src/tailwind-from.css -o ./src/tailwind-to.css && vite build --watch",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview"
	},
	"dependencies": {
		"@amap/amap-jsapi-loader": "^1.0.1",
		"@ant-design/icons": "^5.4.0",
		"@react-oauth/google": "^0.12.1",
		"@zhangleilaoge/vue-code-converter": "^0.0.9",
		"ahooks": "^3.8.1",
		"alova": "^3.0.8",
		"antd": "^5.19.3",
		"antd-img-crop": "^4.23.0",
		"briar-shared": "^0.0.8",
		"date-fns": "^3.6.0",
		"gapi-script": "^1.2.0",
		"highlight.js": "^11.10.0",
		"lodash-es": "^4.17.21",
		"md5-es": "^1.8.2",
		"prismjs": "^1.29.0",
		"qs": "^6.13.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-markdown": "^9.0.1",
		"react-router-dom": "^6.25.1",
		"rehype-highlight": "^7.0.0",
		"remark-gfm": "^4.0.0",
		"three": "^0.168.0",
		"xlsx-js-style": "^1.2.0"
	},
	"devDependencies": {
		"@tailwindcss/typography": "^0.5.15",
		"@types/lodash-es": "^4.17.12",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@types/three": "^0.168.0",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"concurrently": "^9.0.1",
		"openai": "^4.55.4",
		"rollup": "^4.1.4",
		"rollup-plugin-external-globals": "^0.11.0",
		"rollup-plugin-visualizer": "^5.12.0",
		"sass": "^1.77.8",
		"tailwindcss": "^3.4.13",
		"terser": "^5.31.3",
		"typescript": "^5.2.2",
		"vite": "^5.3.4",
		"vite-plugin-chunk-split": "^0.5.0",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-ejs": "^1.7.0",
		"vite-plugin-mpa": "^1.2.0"
	}
}
